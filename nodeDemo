var express = require('express');
var app = express();

//app.set('view engine','jade');
//app.set('port',8888);

app.get('/',function(req,res){
  res.render('index',{title:'imooc'});
})

// 监听 8888 端口，开启服务器
app.listen(8888, function (err) {
    if (err) {
        console.log(err);
        return;
    }
    console.log('Listening at http://localhost:8888');
}
